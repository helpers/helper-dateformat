{
  "name": "helper-dateformat",
  "description": "Template helper for adding formatted dates using node-dateformat. Works with Handlebars, Lo-Dash, underscore, or any template engine that supports helper functions. Also compatible with verb, assemble and Template.",
  "version": "0.1.0",
  "homepage": "https://github.com/helpers/helper-dateformat",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/helper-dateformat",
  "bugs": {
    "url": "https://github.com/helpers/helper-dateformat/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "dateformat": "^2.0.0",
    "kind-of": "^3.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "handlebars": "^4.0.6",
    "lodash": "^2.4.1",
    "mocha": "^3.2.0",
    "verb-generate-readme": "^0.2.0"
  },
  "keywords": [
    "assemble",
    "dateformat",
    "handlebars",
    "helper",
    "helpers",
    "lo-dash",
    "lodash",
    "render",
