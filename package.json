        "helper-date"
      ]
    },
    "reflinks": [
      "generate",
      "generate-helper",
      "verb",
      "verb-generate-readme",
      "assemble",
      "handlebars",
      "lodash",
      "templates",
      "underscore"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
